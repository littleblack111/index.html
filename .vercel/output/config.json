{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/ghrepos$",
      "dest": "/pages/repo/github/repos.html",
      "check": true
    },
    {
      "src": "^/about$",
      "dest": "/pages/info/about.html",
      "check": true
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    }
  ],
  "crons": [],
  "flags": []
}
